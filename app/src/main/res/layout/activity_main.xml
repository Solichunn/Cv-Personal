            android:textStyle="bold"
            android:textSize="13sp"
            android:textColor="#5f9ea0"
            android:layout_marginStart="230dp"
            android:layout_marginBottom="-30dp"
            android:layout_alignBottom="@id/pengalaman"/>
        <TextView
            android:id="@+id/isipengalaman1"
            android:layout_width="160dp"
            android:layout_height="30dp"
            android:text="@string/pengalaman1"
            android:textSize="13sp"
            android:textColor="#000"
            android:layout_marginStart="230dp"
            android:layout_marginBottom="-20dp"
            android:layout_alignBottom="@id/pengalaman1"/>
        <TextView
            android:id="@+id/pengalaman2"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:text="STUDIO SHODWE"
            android:textStyle="bold"
            android:textSize="13sp"
            android:textColor="#5f9ea0"
            android:layout_marginStart="230dp"
            android:layout_marginBottom="-90dp"
            android:layout_alignBottom="@id/pengalaman1"/>
        <TextView
            android:id="@+id/isipengalaman2"
