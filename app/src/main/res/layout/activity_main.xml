            android:layout_marginStart="200dp"
            android:layout_marginTop="170dp"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="#000"/>
        <TextView
            android:id="@+id/profil"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:text="PROFIL"
            android:textStyle="bold"
            android:textSize="20sp"
            android:textColor="#000"
            android:layout_marginStart="20dp"
            android:layout_marginTop="250dp"/>
        <TextView
            android:id="@+id/isiprofil"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:text="@string/isiprofil"
            android:textSize="13sp"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="-100dp"
            android:layout_alignBottom="@id/profil"/>
        <TextView
            android:id="@+id/edukasi"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:text="EDUKASI"
            android:textStyle="bold"
            android:textColor="#000"
            android:textSize="20sp"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="-40dp"
            android:layout_alignBottom="@id/isiprofil"/>
        <TextView
            android:id="@+id/edu1"
            android:layout_width="200dp"
            android:layout_height="20dp"
            android:text="2008 SMA Mandalahayu Bekasi"
            android:textSize="12sp"
            android:textStyle="bold"
            android:textColor="#000"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="-20dp"
