            android:textSize="12sp"
            android:textStyle="bold"
            android:textColor="#000"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="-20dp"
            android:layout_alignBottom="@id/edu2"/>
        <ImageView
            android:id="@+id/iconhp"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/wa"
            android:layout_alignBottom="@id/edu3"
            android:layout_marginBottom="-50dp"
            android:layout_marginStart="20dp"/>
        <TextView
            android:id="@+id/nohp"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:text="08979658837"
            android:layout_alignBottom="@id/edu3"
            android:layout_marginStart="70dp"
            android:layout_marginBottom="-50dp"
            android:textColor="#000"
            android:textStyle="bold"/>
        <ImageView
            android:id="@+id/iconemail"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/email"
            android:layout_alignBottom="@id/iconhp"
            android:layout_marginBottom="-50dp"
            android:layout_marginStart="20dp"/>
        <TextView
            android:id="@+id/isiemail"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:text="mazz.dan567@gmail.com"
            android:layout_alignBottom="@id/nohp"
            android:layout_marginStart="70dp"
            android:layout_marginBottom="-45dp"
            android:textColor="#000"
            android:textStyle="bold"/>
        <ImageView
            android:id="@+id/iconlokasi"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/lokasi"
            android:layout_alignBottom="@id/iconemail"
            android:layout_marginBottom="-50dp"
            android:layout_marginStart="20dp"/>
