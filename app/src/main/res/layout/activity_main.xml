            android:textSize="13sp"
            android:textColor="#5f9ea0"
            android:layout_marginStart="230dp"
            android:layout_marginBottom="-90dp"
            android:layout_alignBottom="@id/pengalaman2"/>
        <TextView
            android:id="@+id/isipengalaman3"
            android:layout_width="160dp"
            android:layout_height="30dp"
            android:text="@string/pengalaman3"
            android:textSize="13sp"
            android:textColor="#000"
            android:layout_marginStart="230dp"
            android:layout_marginBottom="-20dp"
            android:layout_alignBottom="@id/pengalaman3"/>
        <TextView
            android:id="@+id/bahasa"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:text="BAHASA"
            android:textStyle="bold"
            android:textSize="20sp"
            android:textColor="#000"
            android:layout_marginStart="230dp"
            android:layout_marginBottom="-50dp"
            android:layout_alignBottom="@id/isipengalaman3"/>
